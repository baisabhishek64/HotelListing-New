{
  "SeriLog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "./logs/log-.txt",
          "RollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl":  "http://localhost:5341"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
